  "scripts": {
    "build": "npm run clean && npm run build:presentation && npm run cp",
    "build:dev": "npm run build:presentation:dev && npm run cp",
    "build:dev:small": "npm run build:presentation:dev",
    "build:presentation": "cross-env NODE_ENV=production webpack --config webpack.presentation.js --color -p --progress",
    "build:presentation:dev": "cross-env NODE_ENV=development webpack --config webpack.presentation.js --color --progress",
    "clean": "rimraf build",
    "cp": "npm run cp:assets",
    "cp:assets": "mkdir -p ../presentation/assets && ncp presentation/index.html ../presentation/index.html && ncp presentation/assets ../presentation/assets",
    "nsp": "nsp check"
  },
